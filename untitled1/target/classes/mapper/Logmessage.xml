<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.tx.mapper.Logmessage">
    <insert id="addlog" parameterType="cn.tx.pojo.log">

        INSERT INTO log (systemname,mechine, ip,browser) value (#{systemname},#{mechine}, #{ip},#{browser})
    </insert>

</mapper>